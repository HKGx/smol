

fn factorial(n: int) int do
    mut let out = 1
    for v in 1..n+1 do
        let out = out * v
    end
    out
end

fn rec_factorial(n: int) int do
    if n = 1: 1
    else if n = 0: 1
    else:
        n * rec_factorial(n-1)
end

print("iterative: " + str(factorial(10)))
print("recursive: " + str(rec_factorial(10)))